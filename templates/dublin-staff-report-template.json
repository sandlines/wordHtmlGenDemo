{
  "id": "dublin-staff-report-template",
  "name": "Dublin Staff Report",
  "description": "Official staff report template for Dublin City Council with professional municipal formatting",
  "category": "staff-report",
  "layout": "single-column",
  "styling": {
    "fonts": {
      "primary": "Arial, sans-serif",
      "secondary": "Arial, sans-serif",
      "heading": "Arial, sans-serif"
    },
    "colors": {
      "primary": "#0f766e",
      "secondary": "#374151",
      "accent": "#14b8a6",
      "background": "#ffffff",
      "text": "#111827",
      "border": "#d1d5db"
    },
    "spacing": {
      "section": "1.5rem",
      "paragraph": "0.75rem",
      "margin": "1rem",
      "padding": "0.75rem"
    },
    "borders": {
      "width": "1px",
      "style": "solid",
      "radius": "0.25rem"
    },
    "pageSize": "letter",
    "orientation": "portrait",
    "margins": {
      "top": "1in",
      "right": "1in",
      "bottom": "1in",
      "left": "1in"
    }
  },
  "sections": [
    {
      "id": "header",
      "type": "header",
      "name": "Report Header",
      "order": 0,
      "visible": true,
      "content": {
        "type": "doc",
        "content": [
          {
            "type": "heading",
            "attrs": { "level": 1 },
            "content": [
              {
                "type": "text",
                "text": "CITY OF DUBLIN"
              }
            ]
          },
          {
            "type": "heading",
            "attrs": { "level": 2 },
            "content": [
              {
                "type": "text",
                "text": "STAFF REPORT"
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "marks": [{"type": "bold"}],
                "text": "TO:"
              },
              {
                "type": "text",
                "text": " {{recipient}}"
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "marks": [{"type": "bold"}],
                "text": "FROM:"
              },
              {
                "type": "text",
                "text": " {{author_name}}, {{author_title}}"
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "marks": [{"type": "bold"}],
                "text": "DATE:"
              },
              {
                "type": "text",
                "text": " {{report_date}}"
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "marks": [{"type": "bold"}],
                "text": "SUBJECT:"
              },
              {
                "type": "text",
                "text": " {{subject}}"
              }
            ]
          }
        ]
      },
      "variables": ["recipient", "author_name", "author_title", "report_date", "subject"]
    },
    {
      "id": "executive-summary",
      "type": "content",
      "name": "Executive Summary",
      "order": 1,
      "visible": true,
      "content": {
        "type": "doc",
        "content": [
          {
            "type": "heading",
            "attrs": { "level": 3 },
            "content": [
